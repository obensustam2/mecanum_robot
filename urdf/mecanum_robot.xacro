<!-- .............Robot Footprint & Chassis................................................... -->

  <link name="robot_footprint"></link>

  <link
    name="chassis">
    <inertial>
      <origin
        xyz="-8.294E-05 -7.7011E-11 -0.023972"
        rpy="0 0 0" />
      <mass
        value="6.5996" />
      <inertia
        ixx="0.043356"
        ixy="-1.6809E-08"
        ixz="-1.7057E-07"
        iyy="0.13461"
        iyz="3.8673E-11"
        izz="0.17533" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://mecanum_robot/meshes/chassis.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://mecanum_robot/meshes/chassis.STL" />
      </geometry>
    </collision>
  </link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

